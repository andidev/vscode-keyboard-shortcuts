// Place your key bindings in this file to override the defaults
[
  {
    "key": "alt+i",
    "command": "composerMode.agent"
  },
  {
    "key": "alt+escape",
    "command": "-editor.action.triggerSuggest",
    "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly && !suggestWidgetVisible"
  },
  {
    "key": "alt+k",
    "command": "workbench.action.lastEditorInGroup",
    "when": "!terminalFocus"
  },
  {
    "key": "alt+k",
    "command": "workbench.view.scm",
    "when": "!terminalFocus && workbench.scm.active && activeViewlet != 'workbench.view.scm'"
  },
  {
    "key": "alt+k",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "!terminalFocus && activeViewlet == 'workbench.view.scm'"
  },
  {
    "key": "alt+k",
    "command": "-git.commitAll",
    "when": "!terminalFocus && !inDebugMode && !operationInProgress"
  },
  {
    "key": "alt+down",
    "command": "git.openFile",
    "when": "config.git.enabled && isInDiffEditor"
  },
  {
    "key": "up",
    "command": "editor.action.previousMatchFindAction",
    "when": "findWidgetVisible && (editorFocus && findInputFocussed)"
  },
  {
    "key": "shift+cmd+g",
    "command": "-editor.action.previousMatchFindAction",
    "when": "findWidgetVisible && (editorFocus && findInputFocussed)"
  },
  {
    "key": "down",
    "command": "editor.action.nextMatchFindAction",
    "when": "findWidgetVisible && (editorFocus && findInputFocussed)"
  },
  {
    "key": "cmd+g",
    "command": "-editor.action.nextMatchFindAction",
    "when": "findWidgetVisible && (editorFocus && findInputFocussed)"
  },
  {
    "key": "up",
    "command": "workbench.action.terminal.findPrevious",
    "when": "terminalFindFocused && terminalFindVisible"
  },
  {
    "key": "shift+cmd+g",
    "command": "-workbench.action.terminal.findPrevious",
    "when": "terminalFindFocused && terminalFindVisible"
  },
  {
    "key": "down",
    "command": "workbench.action.terminal.findNext",
    "when": "terminalFindFocused && terminalFindVisible"
  },
  {
    "key": "cmd+g",
    "command": "-workbench.action.terminal.findNext",
    "when": "terminalFindFocused && terminalFindVisible"
  },
  {
    "key": "alt+w",
    "command": "workbench.action.closeOtherEditors"
  },
  {
    "key": "alt+cmd+t",
    "command": "-workbench.action.closeOtherEditors"
  },
  {
    "key": "alt+c",
    "command": "cursorai.action.generateInTerminal",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported || terminalHasBeenCreated && terminalPromptBarVisible || terminalProcessSupported && terminalPromptBarVisible"
  },
  {
    "key": "ctrl+k",
    "command": "-cursorai.action.generateInTerminal",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported || terminalHasBeenCreated && terminalPromptBarVisible || terminalProcessSupported && terminalPromptBarVisible"
  },
  {
    "key": "cmd+k",
    "command": "-cursorai.action.generateInTerminal",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported || terminalHasBeenCreated && terminalPromptBarVisible || terminalProcessSupported && terminalPromptBarVisible"
  },
  {
    "key": "shift+cmd+p",
    "command": "projectManager.listProjectsNewWindow"
  },
  {
    "key": "alt+cmd+p",
    "command": "-projectManager.listProjects"
  },
  {
    "key": "shift+alt+cmd+p",
    "command": "projectManager.listProjects"
  },
  {
    "key": "shift+cmd+p",
    "command": "-workbench.action.showCommands"
  },
  {
    "key": "alt+cmd+w",
    "command": "workbench.action.closeWindow"
  },
  {
    "key": "shift+cmd+w",
    "command": "-workbench.action.closeWindow"
  },
  {
    "key": "shift+alt+o",
    "command": "workbench.action.quickOpen"
  },
  {
    "key": "ctrl+e",
    "command": "-workbench.action.quickOpen"
  },
  {
    "key": "ctrl+p",
    "command": "-workbench.action.quickOpen"
  },
  {
    "key": "shift+alt+a",
    "command": "workbench.action.showCommands"
  },
  {
    "key": "ctrl+shift+p",
    "command": "-workbench.action.showCommands"
  },
  {
    "key": "ctrl+g",
    "command": "editor.action.addSelectionToNextFindMatch",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+d",
    "command": "-editor.action.addSelectionToNextFindMatch",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+g",
    "command": "notebook.addFindMatchToSelection",
    "when": "config.notebook.multiCursor.enabled && notebookCellEditorFocused && activeEditor == 'workbench.editor.notebook'"
  },
  {
    "key": "ctrl+d",
    "command": "-notebook.addFindMatchToSelection",
    "when": "config.notebook.multiCursor.enabled && notebookCellEditorFocused && activeEditor == 'workbench.editor.notebook'"
  },
  {
    "key": "ctrl+shift+g",
    "command": "cursorUndo",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+u",
    "command": "-cursorUndo",
    "when": "textInputFocus"
  },
  {
    "key": "alt+oem_5",
    "command": "workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "ctrl+oem_3",
    "command": "-workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "ctrl+t",
    "command": "workbench.action.terminal.new",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+shift+oem_3",
    "command": "-workbench.action.terminal.new",
    "when": "terminalProcessSupported || terminalWebExtensionContributedProfile"
  },
  {
    "key": "ctrl+shift+tab",
    "command": "workbench.action.terminal.focusPrevious",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+tab",
    "command": "workbench.action.terminal.focusNext",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+w",
    "command": "runCommands",
    "args": {
      "commands": [
        "workbench.action.terminal.killActiveTab",
        "workbench.action.terminal.focus"
      ]
    },
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+d",
    "command": "editor.action.duplicateSelection",
    "when": "editorFocus"
  },
  {
    "key": "shift+alt+7",
    "command": "editor.action.commentLine",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+oem_2",
    "command": "-editor.action.commentLine",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+alt+backspace",
    "command": "workbench.action.navigateToLastEditLocation"
  },
  {
    "key": "ctrl+w",
    "command": "-workbench.action.terminal.killEditor",
    "when": "terminalEditorFocus && terminalFocus && terminalHasBeenCreated || terminalEditorFocus && terminalFocus && terminalProcessSupported"
  },
  {
    "key": "ctrl+e",
    "command": "-cursor.toggleAgentWindowIDEUnification",
    "when": "cursor.agentIdeUnification.featureGate && workbenchState != 'empty'"
  },
  {
    "key": "alt+1",
    "command": "workbench.view.explorer",
    "when": "!filesExplorerFocus && viewContainer.workbench.view.explorer.enabled"
  },
  {
    "key": "alt+1",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "sideBarVisible && filesExplorerFocus && !isAuxiliaryWindowFocusedContext"
  },
  {
    "key": "ctrl+shift+e",
    "command": "-workbench.action.quickOpenNavigatePreviousInFilePicker",
    "when": "inFilesPicker && inQuickOpen"
  },
  {
    "key": "ctrl+shift+e",
    "command": "-workbench.view.explorer",
    "when": "viewContainer.workbench.view.explorer.enabled"
  },
  {
    "key": "ctrl+shift+e",
    "command": "-workbench.action.quickOpenNavigatePreviousInFilePicker",
    "when": "inFilesPicker && inQuickOpen"
  },
  {
    "key": "ctrl+b",
    "command": "-workbench.action.toggleSidebarVisibility",
    "when": "!isAuxiliaryWindowFocusedContext"
  },
  {
    "key": "alt+2",
    "command": "workbench.view.search",
    "when": "!searchViewletFocus && viewContainer.workbench.view.explorer.enabled"
  },
  {
    "key": "alt+2",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "sideBarVisible && searchViewletFocus && !isAuxiliaryWindowFocusedContext"
  },
  {
    "key": "alt+3",
    "command": "workbench.view.scm",
    "when": "!(sideBarFocus && activeViewlet == 'workbench.view.scm') && viewContainer.workbench.view.explorer.enabled"
  },
  {
    "key": "alt+3",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "sideBarVisible && sideBarFocus && activeViewlet == 'workbench.view.scm' && !isAuxiliaryWindowFocusedContext"
  },
  {
    "key": "alt+4",
    "command": "workbench.view.debug",
    "when": "!(sideBarFocus && activeViewlet == 'workbench.view.debug') && viewContainer.workbench.view.debug.enabled"
  },
  {
    "key": "alt+4",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "sideBarVisible && sideBarFocus && activeViewlet == 'workbench.view.debug' && !isAuxiliaryWindowFocusedContext"
  },
  {
    "key": "alt+5",
    "command": "workbench.view.extensions",
    "when": "!(sideBarFocus && activeViewlet == 'vscode1.focusedViewlet == 'workbench.view.extensions'') && viewContainer.workbench.view.extensions.enabled"
  },
  {
    "key": "alt+5",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "sideBarVisible && sideBarFocus && activeViewlet == 'workbench.view.extensions' && !isAuxiliaryWindowFocusedContext"
  }
]
